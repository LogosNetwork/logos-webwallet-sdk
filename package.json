{
  "name": "@logosnetwork/logos-webwallet-sdk",
  "version": "0.2.19",
  "description": "Create Logos wallets without a full Logos node",
  "keywords": [
    "Logos",
    "WebWallet",
    "wallet",
    "cryptocurrency",
    "light",
    "lightwallet"
  ],
  "homepage": "https://github.com/LogosNetwork/logos-webwallet-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LogosNetwork/logos-webwallet-sdk.git"
  },
  "main": "./src/index",
  "types": "./typings/index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run lint:typings && npm run unit:test",
    "unit:test": "./node_modules/.bin/mocha --reporter spec",
    "lint": "eslint src *.js",
    "lint:fix": "eslint --fix src",
    "lint:typings": "tslint typings/index.d.ts",
    "build:browser": "webpack",
    "stats:webpack": "webpack --profile --json > webpack/stats.json",
    "prepublishOnly": "npm run test && NODE_ENV=production npm run build:browser"
  },
  "author": "Tyler Storm <tyler@prometheanlabs.io>",
  "contributors": [
    "Tyler Storm <tyler@pla.bs> (https://logos.network)",
    "Chris Cohoat <chris@cohoat.pizza> (https://cohoat.pizza/)",
    "Jaime Herrero",
    "Mart Roosmaa <mart@roosmaa.net>",
    "Gray Olson <gray@grayolson.com> (https://www.grayolson.me/)",
    "Ilya Apasov"
  ],
  "license": "MIT",
  "unpkg": "./webpack/logoswallet.min.js",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@types/node": "^12.6.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "terser-webpack-plugin": "^1.3.0",
    "tslint": "^5.18.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@logosnetwork/logos-rpc-client": "^1.1.29",
    "assert": "^2.0.0",
    "big-integer": "^1.6.44",
    "blakejs": "^1.1.0",
    "mqtt": "^3.0.0",
    "mqtt-regex": "^1.0.5",
    "pbkdf2": "^3.0.17",
    "tweetnacl": "git+https://git@github.com/jaimehgb/tweetnacl-js.git#ef9ea5fd95c3ca98128877c58e40ef1a711421ef"
